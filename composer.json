{
    "name": "indatus/trucker",
    "description": "A simple active resource implementation written in PHP to consume any REST API",
    "keywords":["trucker", "activeresource", "active resource"],
    "type": "library",
    "homepage": "http://indatus.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "Brian Webb",
            "homepage": "http://indatus.com/",
            "email": "bwebb@indatus.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/brianwebb01/filesystem"
        },
        {
            "type": "vcs",
            "url": "https://github.com/brianwebb01/hot-phpunit-runner"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "guzzle/guzzle": "3.8.*",
        "illuminate/support": "~4",
        "illuminate/config": "~4",
        "illuminate/container": "~4",
        "doctrine/inflector": "1.2.*",
        "patchwork/utf8": "1.3.*"
    },
    "require-dev": {
        "facile-it/paraunit": "^0.10.1",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Trucker\\": "src/Trucker"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Trucker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "stable"
}
